# Project details from CMake.
PROJECT_NAME     = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER   = "@CMAKE_PROJECT_VERSION@"
INPUT            = "@DOXYGEN_INPUT_DIR@"
OUTPUT_DIRECTORY = "@DOXYGEN_OUTPUT_DIR@"

# Redirect build messages.
QUIET        = YES
WARN_LOGFILE = @DOXYGEN_WARN_LOGFILE@

# Generate recursively from root source dir.
RECURSIVE = YES

# Spec commenting style.
JAVADOC_AUTOBRIEF = YES
QT_AUTOBRIEF      = YES

# Generate HTML.
GENERATE_HTML = YES
HTML_OUTPUT   = html

# Generate graphs if dot is installed.
HAVE_DOT     = @DOXYGEN_HAVE_DOT@
DOT_PATH     = @DOXYGEN_DOT_PATH@
CALL_GRAPH   = @DOXYGEN_CALL_GRAPH@
CALLER_GRAPH = @DOXYGEN_CALLER_GRAPH@
